        if player has permission "op":
            if player is sneaking:
                make player execute command "/adminshop"
                remove 1 nether star from player's inventory
                stop
        make player execute command "/shop"
    if held item of player is a iron sword:
        if player has permission "op":
            if player is sneaking:
