    permission: op
    aliases: opshop
    trigger:
        give a nether star to player
        set {_money} to {minijeu::%player's uuid%.money}
        add 64 to {minijeu::%player's uuid%.money}
        make player execute command "/shop"
        wait 20 seconds
        set {minijeu::%player's uuid%.money} to {_money}



